pro jwst_clear_ql_info,info

blankline = '                                                                     '
widget_control,info.jwst_filetag[0] ,set_value = blankline
widget_control,info.jwst_typetag, set_value =blankline

widget_control,info.jwst_filetag[1] ,set_value = blankline
num = n_elements(info.jwst_line_tag)
for i = 0, num -1 do begin
    widget_control,info.jwst_line_tag[i],set_value = blankline
endfor
end
