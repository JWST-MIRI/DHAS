         slabelID            : lonarr(3,8) ,$  ; Stat label (only 3 graphs have statistics)
	 sname               : strarr(10),$
         rlabelID            : lonarr(3,2) ,$; ; range label (only 3 graphs have ranges)- user input 
         mdlabelID           : lonarr(3) ,$  ; median label
	 subarray            : 0,$
