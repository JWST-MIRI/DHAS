pro clear_ql_info,info

blankline = '                                                                     '
widget_control,info.filetag[0] ,set_value = blankline
widget_control,info.typetag, set_value =blankline

widget_control,info.filetag[1] ,set_value = blankline
num = n_elements(info.line_tag)
for i = 0, num -1 do begin
    widget_control,info.line_tag[i],set_value = blankline
endfor
end
